{
  "name": "@mentalspace/backend",
  "version": "2.0.0",
  "description": "Backend API for MentalSpace EHR V2",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@mentalspace/database": "*",
    "@mentalspace/shared": "*",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "compression": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1",
    "winston": "^3.17.0",
    "express-rate-limit": "^7.5.0",
    "dotenv": "^16.4.7",
    "aws-sdk": "^2.1691.0",
    "@aws-sdk/client-secrets-manager": "^3.737.0",
    "@aws-sdk/client-s3": "^3.737.0",
    "@aws-sdk/client-dynamodb": "^3.737.0",
    "@aws-sdk/lib-dynamodb": "^3.737.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.12.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  }
}
